{
    "name": "IS Extension",
    "version": "1.2.0",
    "icons": {
        "180": "icon.png"
    },
    "background": {
        "service_work": "background/background.html"
    },
    "description": "Ferramenta para auxílio na análise de IS's.",
    "action": {
        "default_title": "Análise IS",
        "default_popup": "popup/index.html"
    },
    "content_scripts": [
      {
          "matches": [
              "*://*/*"
          ],
          "js": [
              "chromeUtils.js",
              "content.js"
          ]
      }
    ],
    "permissions": [
        "tabs",
        "storage",
        "sidePanel",
	    "clipboardRead"
    ],
    "side_panel": {
        "default_path": "popup/index.html"
    },
    "manifest_version": 3
}